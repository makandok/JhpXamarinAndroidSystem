            android:layout_width="wrap_content"
            android:visibility="visible"
            android:layout_height="wrap_content"
            android:checked="false"
             android:text="@string/vm_gexam_abnormal_penis_discharge"
             android:id="@+id/vm_gexam_abnormal_penis_discharge" />
 <CheckBox
            android:layout_width="wrap_content"
            android:visibility="visible"
            android:layout_height="wrap_content"
            android:checked="false"
             android:text="@string/vm_gexam_abnormal_penis_epispadiasis"
             android:id="@+id/vm_gexam_abnormal_penis_epispadiasis" />
 <CheckBox
            android:layout_width="wrap_content"
            android:visibility="visible"
            android:layout_height="wrap_content"
            android:checked="false"
             android:text="@string/vm_gexam_abnormal_penis_hypospadiasis"
             android:id="@+id/vm_gexam_abnormal_penis_hypospadiasis" />
 <CheckBox
            android:layout_width="wrap_content"
            android:visibility="visible"
            android:layout_height="wrap_content"
            android:checked="false"
             android:text="@string/vm_gexam_abnormal_penis_other"
             android:id="@+id/vm_gexam_abnormal_penis_other" />

    <TextView
        android:text="@string/vm_ge_otherabnormal_penis"
