             android:id="@+id/vm_gexam_abnormal_penis_phimosis" />
 <CheckBox
            android:layout_width="wrap_content"
            android:visibility="visible"
            android:layout_height="wrap_content"
            android:checked="false"
             android:text="@string/vm_gexam_abnormal_penis_paraphimosis"
             android:id="@+id/vm_gexam_abnormal_penis_paraphimosis" />
 <CheckBox
            android:layout_width="wrap_content"
            android:visibility="visible"
            android:layout_height="wrap_content"
