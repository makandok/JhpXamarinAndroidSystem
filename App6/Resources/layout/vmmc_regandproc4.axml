        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:labelFor="@+id/vm_bp_systolic" />
    <EditText
        android:layout_height="40dp"
        android:layout_width="match_parent"
        android:inputType="number"
        android:id="@+id/vm_bp_systolic"
        android:hint="@string/vm_bp_systolic" />

    <TextView
        android:text="@string/vm_bp_diastolic"
        android:textColor="@android:color/holo_blue_dark"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:labelFor="@+id/vm_bp_diastolic" />
    <EditText
        android:layout_height="40dp"
        android:layout_width="match_parent"
        android:inputType="number"
        android:id="@+id/vm_bp_diastolic"
        android:hint="@string/vm_bp_diastolic" />

    <TextView
        android:text="@string/vm_gexam_hdr"
