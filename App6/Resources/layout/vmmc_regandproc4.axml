        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:labelFor="@+id/vm_ge_otherabnormal_penis" />
    <EditText
        android:layout_height="40dp"
        android:layout_width="match_parent"
        android:inputType="text"
        android:id="@+id/vm_ge_otherabnormal_penis"
        android:hint="@string/vm_ge_otherabnormal_penis" />

    <TextView
