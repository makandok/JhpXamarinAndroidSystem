        android:layout_height="wrap_content"
        android:id="@+id/l_vm_referral_hiv_care" />
<RadioButton
    android:layout_marginStart="10dp"
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:checked="false"
android:text=""
android:id="@+id/vm_referral_hiv_care_yes" />

<RadioButton
    android:layout_marginStart="10dp"
    android:layout_width="100dp"
    android:layout_height="wrap_content"
    android:checked="false"
android:text=""
android:id="@+id/vm_referral_hiv_care_no" /></RadioGroup >

<RadioGroup
android:orientation="horizontal"
